package net.engyne.util;

import java.text.DecimalFormat;  
public class BigDoubleToString {

	public static String doubleToString(double d)  
    {  
        String i = DecimalFormat.getInstance().format(d);  
        String result = i.replaceAll(",", "");  
        return result;  
  
    }  
public static void main(String[] args) {
	Double d=(double) 1469980800;
	System.out.println(BigDoubleToString.doubleToString(d));
}
}
